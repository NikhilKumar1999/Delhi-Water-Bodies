<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <stroke
        android:width="4dp"
        android:color="#FBFBFB" />

    // the inside color
    <solid
        android:color="#F4F0F0"
        />

    <corners android:radius="360dp" />

    <padding
        android:left="10dp"
        android:top="5dp"
        android:right="10dp"
        android:bottom="5dp"    >
    </padding>
    <margin
        android:left="10dp"
        android:right="10dp">
    </margin>
</shape>



